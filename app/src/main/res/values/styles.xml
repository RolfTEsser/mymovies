<resources>

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <!-- Application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="android:windowBackground">@drawable/movies_holo_blau_buch</item>
        <item name="android:autoCompleteTextViewStyle">@style/mySuggestionList</item>
    </style>

    <style name="mySuggestionList" parent="@android:style/Widget.Holo.AutoCompleteTextView">
        <item name="android:popupBackground">@color/suggestionlistcolor</item>
    </style>



    <color name="suggestionlistcolor">#b0666677</color>


</resources>